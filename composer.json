{
    "name": "jedi58/reserializer",
    "type": "library",
    "license": "MIT",
    "authors": [
        { "name": "David G. Paul" }
    ],
    "homepage": "https://github.com/jedi58/reserializer",
    "keywords": ["serialize", "data", "fix", "objects"],
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": { "jedi58\\Reserializer\\": "src/" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "archive": {
        "exclude": [
            ".*.yml", "phpunit.xml", "tests"
        ]
    }
}
